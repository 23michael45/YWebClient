page {
  background: #fff;
}
.index-top {
  position: fixed;
  width: 100%;
  top: 0;
  z-index: 99;
}
.index-bottom {
  padding-bottom: 100rpx;
  padding-top: 100rpx;
  background: #eee;
}
.add {
  margin: 50rpx 0 20rpx 120rpx;
  width: 150rpx;
  height: 150rpx;
  line-height: 150rpx;
  text-align: center;
  background-color: #ccc;
  color: #ffffff;
}
.add .add-font {
  font-size: 100rpx;
}
.tab-item {
  overflow: scroll;
  height: 100%;
  position: relative;
}
.tab-item .tip {
  position: absolute;
  top: 50rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
      -ms-transform: translateX(-50%);
          transform: translateX(-50%);
}
.my-camera {
  position: fixed;
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  bottom: 130rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
      -ms-transform: translateX(-50%);
          transform: translateX(-50%);
}
.my-camera image {
  width: 100%;
  height: 100%;
}

