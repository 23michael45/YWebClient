<view><cu-custom class="seat" vue-id="1" bgColor="bg-white" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">查询列表</view></cu-custom><view class="cu-bar bg-white search "><view class="search-form round"><text class="cuIcon-search"></text><input type="text" placeholder="输入搜索的关键词" confirm-type="search" data-event-opts="{{[['input',[['__set_model',['','seos','$event',[]]]]]]}}" value="{{seos}}" bindinput="__e"/></view><view class="action"><button data-event-opts="{{[['tap',[['queryActivity',['$event']]]]]}}" class="cu-btn bg-gradual-green shadow-blur round" bindtap="__e">搜索</button></view></view><view class="sections"><block wx:for="{{activityList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['update',['$0'],[[['activityList','id',item.id,'id']]]]]]]}}" class="sectiondd" bindtap="__e"><block wx:for="{{item.gcImgList}}" wx:for-item="i" wx:for-index="__i1__" wx:key="id"><block wx:if="{{i.type==3}}"><image style="width:100%;height:318rpx;" src="{{i.url}}"></image></block></block><view class="section_header"><view class="_h3">{{"名称:"+item.name}}</view><view class="_h3">{{"访问:"+(item.activity_upCnt?item.activity_upCnt:0)}}</view><view class="_h3">{{"进店:"+(item.shop_upCnt?item.shop_upCnt:0)}}</view><view class="update _h3" style="visibility:hidden;">收藏:{{}}</view></view><view class="section_footer"><view class="_h3">转发:{{}}</view><view class="_h3">{{"点赞:"+item.upCnt}}</view><view class="_h3">收藏:{{}}</view><view class="update _h3">编辑</view></view></view></block></view></view>