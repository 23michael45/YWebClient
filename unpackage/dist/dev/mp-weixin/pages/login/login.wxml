<view class="login" style="{{'height:'+(height+'px')+';'}}"><cu-custom class="seat" vue-id="1" bgColor="bg-black" isBack="{{false}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">羽迹试试管理后台</view></cu-custom><image src="../../static/img/login.jpg" mode="aspectFill"></image><view class="login-box"><view class="login-item margin-top"><view class="title cuIcon-people"></view><input placeholder="请输入账号" name="input" data-event-opts="{{[['input',[['__set_model',['','ucode','$event',[]]]]]]}}" value="{{ucode}}" bindinput="__e"/></view><view class="login-item"><view class="title cuIcon-lock"></view><input placeholder="请输入密码" password name="input" data-event-opts="{{[['input',[['__set_model',['','pass','$event',[]]]]]]}}" value="{{pass}}" bindinput="__e"/></view><view class="padding"><button data-event-opts="{{[['tap',[['toRegiste',['$event']]]]]}}" class="login-btn cu-btn bg-black" bindtap="__e">注册</button><button class="login-btn cu-btn bg-black" disabled="{{!isTrue}}" type data-event-opts="{{[['tap',[['Login',['$event']]]]]}}" bindtap="__e">登录</button></view><view class="tip"><text>忘记密码？</text></view></view></view>