.service-info .cu-list .cu-item .action.logo {
  position: relative;
  height: 200rpx;
  width: 200rpx;
  border: 1px solid #eee;
  margin: 20rpx 0;
}
.service-info .cu-list .cu-item .action.logo image {
  width: 190rpx;
  height: 190rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
      -ms-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.service-info .cu-list .cu-item .action.logo .add {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
      -ms-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  font-size: 120rpx;
}
.service-info .cu-list .cu-item .action .input {
  text-align: right;
}
.service-info .my-btn {
  text-align: center;
  margin-top: 250rpx;
}

