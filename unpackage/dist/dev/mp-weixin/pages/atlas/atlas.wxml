<view><cu-custom class="seat" vue-id="1" bgColor="bg-white" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">图集</view></cu-custom><view class="atlas"><view class="atlas-content"><view class="atlas-time">2019/09/11</view><view class="atlas-text">{{goodsObj.unit}}</view><block wx:if="{{goodsObj.isCanPrivate==1}}"><view class="atlas-goods"><block wx:for="{{goodsObj.imgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="goods-item"><image src="{{item.url}}" mode="aspectFill" data-event-opts="{{[['tap',[['ChooseImage',['$0'],[[['goodsObj.imgList','',index,'url']]]]]]]}}" bindtap="__e"></image></view></block></view></block><block wx:if="{{goodsObj.isCanPrivate==2}}"><view class="atlas-goods"><video src="{{goodsObj.imgList[0].url}}" controls></video></view></block><view class="atlas-btn-box"><view data-event-opts="{{[['tap',[['del',['$event']]]]]}}" class="atlas-btn" bindtap="__e">删除</view><view data-event-opts="{{[['tap',[['edit',['$event']]]]]}}" class="atlas-btn" bindtap="__e">编辑</view></view><view class="atlas-share">分享</view></view></view></view>