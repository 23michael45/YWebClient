<view class="store"><cu-custom class="seat" vue-id="1" bgColor="bg-white" isBack="{{false}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">我的</view></cu-custom><view class="top"><view class="logo"><view data-event-opts="{{[['tap',[['upLogo',['$event']]]]]}}" class="icon" bindtap="__e"><block wx:if="{{imgurl}}"><image src="{{imgurl}}" mode="aspectFill"></image></block><block wx:else><view class="text text-black">未设置LOGO</view></block></view><view class="text">{{(companyInfo.name?companyInfo.name:'未设置店铺名')+''}}</view></view></view><view class="bottom"><view class="cu-list menu sm-border"><view data-event-opts="{{[['tap',[['toStoreInfo',['$event']]]]]}}" class="cu-item arrow" bindtap="__e"><view class="content"><text class="cuIcon-taoxiaopu text-black"></text><text class="text-black">店铺信息</text></view></view><view class="cu-item arrow"><view class="content"><text class="cuIcon-discoverfill text-black"></text><text class="text-black">活动详情</text></view></view><view data-event-opts="{{[['tap',[['toAttestAtion',['$event']]]]]}}" class="cu-item arrow" bindtap="__e"><view class="content"><text class="cuIcon-selectionfill text-black"></text><text class="text-black">认证信息</text></view></view><view data-event-opts="{{[['tap',[['toServiceInfo',['$event']]]]]}}" class="cu-item arrow" bindtap="__e"><view class="content"><text class="cuIcon-profilefill text-black"></text><text class="text-black">客服信息</text></view></view><view data-event-opts="{{[['tap',[['toFeedBack',['$event']]]]]}}" class="cu-item arrow" bindtap="__e"><view class="content"><text class="cuIcon-profilefill text-black"></text><text class="text-black">功能反馈</text></view></view></view></view></view>