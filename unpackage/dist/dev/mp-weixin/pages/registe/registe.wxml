<view class="registe"><cu-custom class="seat" vue-id="1" bgColor="bg-white" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">注册</view></cu-custom><form><view class="cu-form-group margin-top"><view class="title">账号：</view><input placeholder="输入账号(店铺名称)" placeholder-class="placeholder" name="ucode" data-event-opts="{{[['input',[['__set_model',['','ucode','$event',[]]]]]]}}" value="{{ucode}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">密码：</view><input placeholder="输入密码" placeholder-class="placeholder" password name="pass" data-event-opts="{{[['input',[['__set_model',['','pass','$event',[]]]]]]}}" value="{{pass}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">确认密码：</view><input placeholder="再次输入密码" placeholder-class="placeholder" password name="againPass" data-event-opts="{{[['input',[['__set_model',['','againPass','$event',[]]]]]]}}" value="{{againPass}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">公司名称：</view><input placeholder="输入公司名称" placeholder-class="placeholder" name="name" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">公司执照编码：</view><input placeholder="输入公司执照编码" placeholder-class="placeholder" name="registerCode" data-event-opts="{{[['input',[['__set_model',['','registerCode','$event',[]]]]]]}}" value="{{registerCode}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">公司法人：</view><input placeholder="输入公司法人" placeholder-class="placeholder" name="charger" data-event-opts="{{[['input',[['__set_model',['','charger','$event',[]]]]]]}}" value="{{charger}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">公司地址：</view><input placeholder="输入公司地址" placeholder-class="placeholder" name="addr" data-event-opts="{{[['input',[['__set_model',['','addr','$event',[]]]]]]}}" value="{{addr}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">手机号码：</view><input placeholder="输入手机号码" placeholder-class="placeholder" maxlength="11" type="number" name="tel" data-event-opts="{{[['input',[['__set_model',['','tel','$event',[]]]]]]}}" value="{{tel}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">验证码：</view><input placeholder="输入验证码" placeholder-class="placeholder" type="number" maxlength="6" name="code" data-event-opts="{{[['input',[['__set_model',['','code','$event',[]]]]]]}}" value="{{code}}" bindinput="__e"/><button class="cu-btn bg-green shadow" type disabled="{{isDis}}" data-event-opts="{{[['tap',[['getCode',['$event']]]]]}}" bindtap="__e">{{timerInfo}}</button></view><view class="agreement"><text data-event-opts="{{[['tap',[['agree',['$event']]]]]}}" class="{{['round',isAgree?'cuIcon-roundcheck':'cuIcon-round']}}" bindtap="__e"><text>阅读并同意以下协议</text></text><text class="agreement-text">《服务协议》</text></view><view class="registe-btn"><button data-event-opts="{{[['tap',[['registe',['$event']]]]]}}" class="{{['cu-btn block bg-blue margin-tb-sm lg',isAgree?'':'opt']}}" bindtap="__e">注册</button></view></form></view>