.video-play {
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  background: #000;
  z-index: 9999;
}
.video-play .video {
  width: 100%;
  height: 100%;
}
.video-play .back {
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  width: 100%;
  padding-left: 20rpx;
  height: 50rpx;
  line-height: 50rpx;
  background: #fff;
  z-index: 99;
}

